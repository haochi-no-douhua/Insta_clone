
<div class="wrap">
    <% @blogs.each do |blog| %>
      <div class="item">
        <% if blog.image.url.present? %>
          <%= link_to blog_path(blog.id) do %>
            <div class="uploaded_image">
              <%= image_tag blog.image.url %>
            </div>
          <% end %>
        <% end %>
      </div>
    <% end %>
</div>
